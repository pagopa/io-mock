{
  "name": "io-mock",
  "version": "1.0.0",
  "repository": "git@github.com:pagopa/io-mock.git",
  "license": "MIT",
  "scripts": {
    "clean": "shx rm -rf generated-env && shx mkdir generated-env",
    "generate": "npm-run-all clean generate:*",
    "generate:env": "node bin/index.js .env generated-env/.env",
    "generate:env-functions-app": "dotenv -e generated-env/.env envsub env.io-functions-app generated-env/env.io-functions-app",
    "generate:env-functions-public": "dotenv -e generated-env/.env envsub env.io-functions-public generated-env/env.io-functions-public",
    "generate:env-functions-admin": "dotenv -e generated-env/.env envsub env.io-functions-admin generated-env/env.io-functions-admin",
    "generate:env-functions-services": "dotenv -e generated-env/.env envsub env.io-functions-services generated-env/env.io-functions-services",
    "generate:env-functions-bonus": "dotenv -e generated-env/.env envsub env.io-functions-bonus generated-env/env.io-functions-bonus",
    "generate:env-backend": "dotenv -e generated-env/.env envsub env.io-backend generated-env/env.io-backend",
    "generate:env-notification-hub": "dotenv -e generated-env/.env envsub env.io-notification-hub generated-env/env.io-notification-hub",
    "generate:env-pagopa-proxy": "dotenv -e generated-env/.env envsub env.io-pagopa-proxy generated-env/env.io-pagopa-proxy",
    "generate:env-pagopa-nodo-mock": "dotenv -e generated-env/.env envsub env.io-pagopa-nodo-mock generated-env/env.io-pagopa-nodo-mock",
    "generate:env-fixtures": "dotenv -e generated-env/.env envsub env.io-fixtures generated-env/env.io-fixtures",
    "generate:env-cosmosdb": "dotenv -e generated-env/.env envsub env.cosmosdb generated-env/env.cosmosdb",
    "prestart": "npm-run-all generate",
    "start": "docker-compose --env-file generated-env/.env up --build",
    "stop": "docker-compose down"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "dotenv-cli": "^3.2.0",
    "envsub": "^4.0.7",
    "node-fetch": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "shx": "^0.3.2"
  }
}
